{
  "name": "cloudflare-workers-types-esm",
  "version": "3.15.0",
  "description": "Automatically generated Cloudflare Workers Types as ES Module",
  "main": "dist/cloudflare-workers-types.ts",
  "module": "dist/cloudflare-workers-types.ts",
  "types": "dist/cloudflare-workers-types.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "ts-node-esm ./build.ts",
    "lint": "eslint --quiet src/**/*",
    "format": "prettier --loglevel warn -c .",
    "format:fix": "prettier --loglevel warn -w .",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles",
    "typecheck": "tsc --noEmit"
  },
  "author": "thatmattlove <matt@stunninglyclear.com>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@octokit/rest": "^19.0.4",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "compare-versions": "^5.0.1",
    "esbuild": "^0.15.8",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.23.1",
    "eslint-plugin-functional": "^4.4.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "extract-zip": "^2.0.1",
    "jest": "^29.0.3",
    "octokit": "^2.0.7",
    "prettier": "^2.7.1",
    "tempy": "^3.0.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.0.0",
    "typescript": "^4.8.3"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "semi": true
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ]
  },
  "eslintConfig": {
    "extends": "plugin:unicorn/recommended",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "project": "tsconfig.json",
      "sourceType": "module"
    },
    "ignorePatterns": [
      "node_modules",
      "dist",
      "jest.config.ts",
      "*.spec.ts",
      "*.test.ts",
      "build.[jt]s"
    ],
    "plugins": [
      "eslint-plugin-unicorn",
      "@typescript-eslint",
      "functional"
    ],
    "rules": {
      "unicorn/prevent-abbreviations": [
        "error",
        {
          "replacements": {
            "temp": false
          }
        }
      ]
    },
    "root": true
  },
  "default": {
